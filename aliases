# vim
alias n='nvim'
alias nb='nvim -u ~/.config/nvim/basic.vim'

# Cheatsheets
alias vimch='nvim ~/Documents/code/cheatsheets/vim.md'
alias tmuxch='nvim ~/Documents/code/cheatsheets/tmux.md'

# Tmux
alias tl='tmux ls'
alias ta='tmux attach -t'
alias tn='tmux new-session -s'
alias tk='tmux kill-session -t'

# Javascript alias
alias pkg='cat package.json'

# Docker
alias dcu='docker-compose up'
alias dcub='docker-compose up --build'
alias dcd='docker-compose down'

# Apps
alias jr='nb "/Users/menor/iCloud/journal/."'
